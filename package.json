{
  "name": "postcss-scopeify-everything",
  "version": "0.3.0",
  "description": "PostCSS plugin to scopify every CSS selector",
  "main": "dist/index.js",
  "repository": "git@github.com:neurosnap/postcss-scopeify-everything.git",
  "scripts": {
    "test": "tape-dots test.js",
    "prepublish": "NODE_ENV=production browserify --standalone postcss-scopeify-everything -t babelify index.js -o dist/index.js",
    "clean": "rm dist/*.js"
  },
  "keywords": [
    "scope",
    "scopify",
    "scopeify",
    "everything",
    "selector",
    "css",
    "class",
    "element",
    "id",
    "keyframes",
    "font-face",
    "postcss",
    "plugin"
  ],
  "author": "Eric Bower",
  "license": "MIT",
  "dependencies": {
    "postcss-safe-parser": "^2.0.0"
  },
  "peerDependencies": {
    "postcss": "5.x"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "postcss": "^5.2.0",
    "tape": "^4.6.0",
    "tape-dots": "^1.0.0"
  }
}
